      "Calculate3DX": {
        "parameters": [
          {
            "name": "radian",
            "default": "@RADIAN_PER_FRAME"
          },
          {
            "name": "cosine",
            "default": "${Math.cos(radian)}"
          },
          {
            "name": "sine",
            "default": "${Math.sin(radian)}"
          },
          "propertyX",
          "propertyZ",
          "x",
          "z"
        ],
        "commands": [
          {
            "type": "SetValue",
            "property": "${propertyX}",
            "value": "${((z - @CUBE_Z) * sine + (x - @CUBE_X) * cosine) + @CUBE_X}"
          },
          {
            "type": "SetValue",
            "property": "${propertyZ}",
            "value": "${((z - @CUBE_Z) * cosine - (x - @CUBE_X) * sine) + @CUBE_Z}"
          }
        ]
      },
